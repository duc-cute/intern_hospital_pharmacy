.custom-scroll {
  &::-webkit-scrollbar,
  &::-webkit-scrollbar-thumb {
    height: 10px;
    width: 10px;
    border: 2px solid transparent;
    background-color: $green;
  }

  &::-webkit-scrollbar-thumb {
    box-shadow: inset 0 0 0 10px;
    color: #aaa;
    border-radius: 10px;
  }
}

.list-product {

  .items-product { 
    border: 1px solid transparent;
    user-select: none;

    &:hover {
      border-color: #0070f4;

      .view-product-btn {
        display: block;
      }
    }

    .view-product-btn {
      display: none;
      z-index: 2;

      .view-product-btn-ico {
        color: #0070f4;
        opacity: 0.6;

        &:hover {
          opacity: 1;
        }
      }
    }
  }
}

