}

.globits-paging-v2 {
  &>.MuiInput-root.MuiInputBase-root.MuiAutocomplete-inputRoot{
    margin-top: 12px;
  }
 .Mui-error{
    color: var(--error) !important;
    & input.MuiAutocomplete-input {
      color: var(--text-body) !important;
    }
    &::after{
      border-bottom-color:  var(--error) !important;
    }
  }

}

.globits-paging-v2 input{
  height: 1.1876em;
}

.input-focus-primary::after:not(.Mui-error){
  border-bottom: 2px solid $primary !important;
} 

.MuiInputBase-root:not(.Mui-error).MuiInput-underline::after{
  border-bottom: 2px solid #006778 !important;

}

.MuiInputBase-root.MuiOutlinedInput-root.Mui-focused.Mui-focused:not(.Mui-error) fieldset {
  border-color: #006778 !important;
}

.MuiFormLabel-root.Mui-focused:not(.Mui-error) {
  color:  #006778 !important;
}

