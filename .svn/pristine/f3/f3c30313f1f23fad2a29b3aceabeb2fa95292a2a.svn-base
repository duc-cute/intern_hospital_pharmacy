.tablePurchaseOrder {
  border-radius: 3px;
  // .MuiTableRow-head {
  //   border-top: #cce2fd;
  //   border-bottom: #cce2fd;
  // }
  thead th {
    font-size: 13px !important;
    white-space: nowrap;
    border: none;
    padding: 4px !important;
  }
  td {
    font-size: 13px !important;
    border: none;
    padding: 0 4px !important;
  }
  &.table-container .row-table-body {
    td {
      background-color: #fff;
    }
  }
  tr {
    cursor: pointer;
    &:hover {
      background-color: #f2fbf5;
      .btn-quantity {
        visibility: visible;
      }
    }
  }
  .input-sales-order {
    .MuiInput-root {
      background-color: unset;
    }
    input {
      font-size: 14px;
    }
    .input-sales-order-max-width {
      max-width: unset;
    }
  }
  .btn-quantity {
    border: 1px solid #999;
    background-color: unset;
    display: flex;
    align-items: center;
    visibility: hidden;
    cursor: pointer;
    &:hover {
      border: 1px solid #333;
      .MuiSvgIcon-root {
        color: #333;
      }
    }
    .MuiSvgIcon-root {
      color: #999;
      width: 20px;
      height: 20px;
    }
  }

}

